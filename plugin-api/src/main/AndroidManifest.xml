<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.volcengine.zeus.plugin_api">

    <application>
        <meta-data
            android:name="ZEUS_PLUGIN_${zeusPluginPkgName}"
            android:value="{
                apiVersionCode:${zeusApiVersionCode},packageName: ${zeusPluginPkgName},
                minPluginVersion:${minPluginVersion},maxPluginVersion:${maxPluginVersion},
                signature:'${signature}',
                internalPath:'${internalPath}',internalVersionCode:${internalVersionCode},
                appKey:'${appKey}',appSecretKey:'${appSecretKey}'
            }" />


        <activity
            android:name="com.volcengine.zeus.plugin_api.StubActivity$AppCompat_main_standard"
            android:configChanges="orientation|keyboardHidden|navigation|screenSize"
            android:exported="false"
            android:launchMode="standard"
            android:windowSoftInputMode="adjustResize|stateHidden" />
    </application>
</manifest>